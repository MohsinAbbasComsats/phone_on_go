
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MasterLayoutPage.cshtml";
}

<style type="text/css">
    @@font-face {
        font-family: Roboto;
        src: url('http://piruby.com/android/Roboto-Regular.ttf');
    }
    .tabs {
        position: absolute;
        left: 0px;
        top: 0px;
        width: 100%;
        height: 50px;
        z-index: 1000;
        background-color: #233445;
        z-index:1;
    }
    .tabopen {
        border-bottom: 5px #3DA0D6 solid;
    }
    .tab {
        font-family: Roboto;
        float: left;
        width: 25% !important;
        height: 100% !important;
        padding-top: 15px;
        color: #FFFFFF;
        text-align: center;
    }
</style>

<h2>Index</h2>
<div class="tabs">
    <div class="tab tabopen">
        Profile
    </div>
    <div class="tab">
        Account
    </div>
</div>



<script type="text/javascript">
    $('.tab').click(function () {
        $('.tabopen').removeClass('tabopen');
        $(this).addClass('tabopen');
    });
</script>

